{
    "name": "opine/form",
    "type": "library",
    "description": "Service for generating form markup to perform CRUD functions",
    "keywords": [
        "MongoDB"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name"    : "Ryan Mahoney",
            "email"   : "ryan@virtuecenter.com",
            "homepage": "http://opine-php.org",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php"             : ">=5.4.0",
        "opine/interfaces": "1.0.0",
        "symfony/yaml"    : "2.6.1",
        "opine/field"     : "dev-master",
        "opine/http"      : "dev-master",
        "opine/layout"    : "dev-master",
        "opine/pubsub"    : "dev-master",
        "opine/container" : "2.0.2",
        "opine/collection": "dev-master",
        "opine/config"    : "3.0.0",
        "opine/db"        : "dev-master",
        "opine/route"     : "3.0.1",
        "opine/bundle"    : "1.9.0",
        "opine/cache"     : "2.0.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "3.7.32"
    },
    "autoload": {
        "psr-4": {
            "Opine\\Form\\": "src/"
        },
        "classmap": [
            "config/forms",
            "config/managers",
            "bundles/Sample/forms",
            "bundles/Sample/managers"
        ]
    }
}