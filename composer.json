{
    "name": "opine\/form",
    "type": "library",
    "description": "Service for generating form markup to perform CRUD functions",
    "keywords": [
        "MongoDB"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ryan Mahoney",
            "email": "ryan@virtuecenter.com",
            "homepage": "http:\/\/opine-php.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phpunit\/phpunit": "3.7.32",
        "opine\/field": "dev-master",
        "opine\/http": "dev-master",
        "opine\/separation": "dev-master",
        "opine\/pubsub": "dev-master",
        "opine\/container": "dev-master",
        "opine\/db": "dev-master",
        "opine\/route": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Opine\\": "src\/"
        },
        "classmap": [
            "forms",
            "managers",
            "bundles/Manager/forms",
            "bundles/Manager/managers",
            "bundles/Sample/forms",
            "bundles/Sample/managers"
        ]
    }
}