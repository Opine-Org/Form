{
    "name": "opine/form",
    "type": "library",
    "description": "Service for generating form markup to perform CRUD functions",
    "keywords": [
        "MongoDB"
    ],
    "license": "MIT",
    "authors": [
        {
            "name"    : "Ryan Mahoney",
            "email"   : "ryan@virtuecenter.com",
            "homepage": "http://opine-php.org",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php"             : ">=5.4.0",
        "symfony/yaml"    : "2.6.1",
        "opine/interfaces": "~1.0",
        "opine/field"     : "~2.0",
        "opine/http"      : "~2.0",
        "opine/layout"    : "~2.0",
        "opine/pubsub"    : "~2.0",
        "opine/container" : "~2.0",
        "opine/collection": "~2.0",
        "opine/config"    : "~3.0",
        "opine/db"        : "~2.0",
        "opine/route"     : "~3.0",
        "opine/bundle"    : "~2.0",
        "opine/cache"     : "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "3.7.32"
    },
    "autoload": {
        "psr-4": {
            "Opine\\Form\\": "src/"
        },
        "classmap": [
            "config/forms",
            "config/managers",
            "bundles/Sample/forms",
            "bundles/Sample/managers"
        ]
    },
    "version": "2.0.8"
}